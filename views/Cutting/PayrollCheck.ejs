    <style>
        /* Modal styles */
        .modal {
          display: none;
          position: fixed;
          z-index: 100;
          left: 0;
          top: 0;
          width: 100%;
          height: 100%;
          overflow: auto;
          background-color: rgba(0,0,0,0.4);
        }
        
        .modal-content {
          background-color: #fefefe;
          margin: 15% auto;
          padding: 20px;
          border: 1px solid #888;
          width: 25%;
        }
        
        /* Close button styles */
        .close, .close-rep, .close-check {
          color: #aaa;
          float: right;
          font-size: 28px;
          font-weight: bold;
        }
        
        .close:hover,
        .close:focus {
          color: black;
          text-decoration: none;
          cursor: pointer;
        }

        .close-rep:hover,
        .close-rep:focus {
          color: black;
          text-decoration: none;
          cursor: pointer;
        }

        .close-check:hover,
        .close-check:focus {
          color: black;
          text-decoration: none;
          cursor: pointer;
        }
        #checkModal table {
        border-collapse: collapse;
        width: 100%;
        }
        #checkModal th, #checkModal td {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }
        #checkModal th {
            background-color: #f2f2f2;
        }
    </style>

