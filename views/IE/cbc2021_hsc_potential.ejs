                var xmlhttp = new XMLHttpRequest();
                xmlhttp.open("post", '/IE/layout_group', true);
                event.preventDefault(); //prevent default action 
                var style = document.getElementById("style_detail").value;
                var login_group=document.getElementById("group_name").value;
                var week_ly = document.getElementById("sel_w_layout").value;
                // if(user!="ngmai1"&&user!="mabach"){
                //     alert("user không được cấp quyền");
                //     return;
                // }
                xmlhttp.onreadystatechange = function () {
                    if (this.readyState == 4 && this.status == 200) {
                      var res_result=this.responseText
                      console.log(res_result)
                      if (res_result=="false"){
                        var c = document.getElementById("myCanvas");
                        var ctx = c.getContext("2d");
                        ctx.clearRect(0, 0, 2192, 900);
                        alert("layout chưa có sẵn")
                      } else{
                        sessionStorage.setItem('res_mc_stt',res_result);
                        Draw_map();
                        dscd=JSON.parse(res_result)
                        if (document.getElementById("sel_w_layout").value=="Current"){
                          document.getElementById("sel_w_layout").value=="Current"
                        } else{
                          document.getElementById("sel_w_layout").value=dscd[0].week;
                        }
                        
                        console.log("tính số lượng máy")
                        for (i=1;i<=11;i++){
                          var idop="opn"+i.toString()
                          var idlay="lay"+i.toString()
                          var slmcyc=0
                          var opname=document.getElementById(idop).innerText
                          for (m=0;m<dscd.length;m++){
                            if (dscd[m].operation==opname){
                              slmcyc=slmcyc+1
                            }
                          }
                          document.getElementById(idlay).innerText=slmcyc.toString();
                        }
                        document.getElementById("applycbc").disabled=false;
                      }
                    }
                };
                // Kiem tra console ra man hinh
                // ===============================================================
                xmlhttp.setRequestHeader("Content-type", "application/json");
                data = {
                    style:style,
                    groupname:login_group,
                    week_layout:week_ly
                };
                console.log(data);
                xmlhttp.send(JSON.stringify(data));
                // xmlhttp.send();
            };



