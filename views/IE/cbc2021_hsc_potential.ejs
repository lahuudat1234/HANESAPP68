      function update_lay(){
          var xmlhttp = new XMLHttpRequest();
          xmlhttp.open("post", '/IE/layout_update', true);
          event.preventDefault(); //prevent default action 
          var style = document.getElementById("style_detail").value;
          var login_group=document.getElementById("group_name").value;
          var week_ly = document.getElementById("sel_week").value;
          var model=document.getElementById("model").value
          var line=document.getElementById("line").value
          var pos=document.getElementById("pos").value
          var tagno=document.getElementById("tagno").value
          var operation=document.getElementById("layout_op").value
          var stts=document.getElementById("login_status").innerText;
          console.log(stts)
          if (operation=="-"||week_ly==0){
            alert("vui lòng kiểm tra lại thông tin công đoạn - tuần")
            return
          }
          if (stts!=" Login_success"||week_ly==0){
            alert("vui lòng kiểm tra lại thông tin tuần cbc hoặc chưa login");
            return
          }
          // if(user!="ngmai1"&&user!="mabach"){
          //     alert("user không được cấp quyền");
          //     return;
          // }
          xmlhttp.onreadystatechange = function () {
              if (this.readyState == 4 && this.status == 200) {
                var res_result=this.responseText
                console.log(res_result)
                if (res_result=="false"){
                  var c = document.getElementById("myCanvas");
                  var ctx = c.getContext("2d");
                  ctx.clearRect(0, 0, 2192, 900);
                  alert("layout chưa có sẵn")
                } else{
                  alert('layout tuần '+week_ly+' đã được cập nhật \n'+line+'-'+pos+' '+operation+' máy '+model)
                  load_layout()
                }
              }
          };
          // Kiem tra console ra man hinh
          // ===============================================================
          xmlhttp.setRequestHeader("Content-type", "application/json");
          data = {
              style:style,
              groupname:login_group,
              week_layout:week_ly,
              model:model,
              line:line,
              pos:pos,
              operation:operation,
              tagno:tagno
          };
          console.log(data);
          xmlhttp.send(JSON.stringify(data));
        }
      
      
