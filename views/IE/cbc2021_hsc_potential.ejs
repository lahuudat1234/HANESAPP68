      function option_line(){
        var groupname=document.getElementById("group_name").value
        var stl=groupname.length
        var fr=parseInt(groupname.substring(0,3))
        var tol=parseInt(groupname.substring(stl-3,stl))
        console.log(fr)
        console.log(tol)

        var x = document.getElementById("line");
        while (x.length >0){
            x.options.remove(0);
            var x = document.getElementById("line");
        }
        while (fr<=tol){
          var x=document.getElementById("line");
          var option = document.createElement("option");
          if (fr<10){
            option.text ="line 00"+fr.toString();
          }
          if (fr<100 && fr>=10){
            option.text ="line 0"+fr.toString();
          }
          if (fr>=100){
            option.text ="line "+fr.toString();
          }
          x.add(option);
          fr=fr+1
        }
      }

