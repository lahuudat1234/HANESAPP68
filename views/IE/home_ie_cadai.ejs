      function hienthioutput(){
        var ds=JSON.parse(sessionStorage.getItem('result_output'));
        $('#table_output tbody').html('')
        var dk=document.getElementById("sel_condition_o").value
        var mhct=document.getElementById("style_detail").value
        var ca=document.getElementById("sel_shift").value
        ca=ca.substr(0,1);
        var i;
        var trHTML_ds='';
        var sr=parseFloat(document.getElementById("sel_r").value);
        var nor=0
        var big=0
        var boy=0
        var sln=0
        var slb=0
        var slboy=0
        if (ds.length==0){
                  alert(" không có dữ liệu output theo điều kiện lọc hiện tại\n vui lòng chọn lại điều kiện lọc\n thank!")
        }
        else{
          if (dk=="Lịch sử toàn bộ theo thời gian"){
            document.getElementById("summary_o").innerText="////"
            for (i = 0; i < ds.length; i++) {
              trHTML_ds += '<tr><td >' + ds[i].date + '</td><td >' + ds[i].shift + '</td><td >' + ds[i].style_detail + '</td><td >' + ds[i].Size +'</td><td >' + Math.round(ds[i].output)+'</td><td >' + ds[i].rate+'</td><td >' + ds[i].RSize+'</td></tr>';
            };

          }
          else{
            for (i = 0; i < ds.length; i++) {
              if (ds[i].style_detail==mhct && parseFloat(ds[i].RSize)>sr && parseFloat(ds[i].rate)>sr && ca==ds[i].shift){
                if(ds[i].Size=="S"||ds[i].Size=="M"||ds[i].Size=="L"||ds[i].Size=="XL"){
                  nor+=1;
                  sln+=Math.round(ds[i].output)
                }
                if(ds[i].Size=="2XL"||ds[i].Size=="3XL"||ds[i].Size=="4XL"){
                  big+=1;
                  slb+=Math.round(ds[i].output)
                }
                if(ds[i].Size=="2"||ds[i].Size=="3"||ds[i].Size=="4"){
                  boy+=1;
                  slboy+=Math.round(ds[i].output)
                }
                trHTML_ds += '<tr><td >' + ds[i].date + '</td><td >' + ds[i].shift + '</td><td >' + ds[i].style_detail + '</td><td >' + ds[i].Size +'</td><td >' + Math.round(ds[i].output)+'</td><td >' + ds[i].rate+'</td><td >' + ds[i].RSize+'</td></tr>';
              }
            };
            var subo='';
            if (nor>0){
              var tbn=Math.round(sln/nor)
              subo+='__Normal size = '+tbn.toString() + ' ////';
            }
            if (big>0){
              var tbn=Math.round(slb/big)
              subo+='__BIG size = '+tbn.toString() + ' ////';
            }
            if (boy>0){
              var tbn=Math.round(slboy/boy)
              subo+='__BOY size = '+tbn.toString() + ' ////';
            }
            document.getElementById("summary_o").innerText=subo;

          }
          $('#table_output tbody').html(trHTML_ds)
        }
      };

