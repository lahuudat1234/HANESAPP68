<script>
    var i = 0;   
    function showProgressBar() {
        document.getElementById("popup-LoadingBar").style.display = "flex";
   
      if (i == 0) {
        i = 1;
        var elem = document.getElementById("LoadingBar");
        var width = 10;
        var id = setInterval(frame, 100);
        function frame() {
          if (width >= 100) {
            clearInterval(id);
            document.getElementById("popup-LoadingBar").style.display = "none";
            i = 0;
            elem.style.width = 1 + "%";
            elem.innerHTML = 1  + "%";
          } else {
            width++;
            elem.style.width = width + "%";
            elem.innerHTML = width  + "%";
          }
        }
      }
     
    
    }

    var startProgressBar = function () {
       if (globalP < 100) {
             //Ensure that it is visible
             $("#progressBar").show();
             globalP = globalP + 4;
             progress(globalP, $('#progressBar'));
             setTimeout(startProgressBar, 500);
       }
       else {
             //It's fininshed
            alert("Done");
            //Set your progress to zero and hide the bar
            globalP = 0;
            progress(globalP, $('#progressBar'));
            $("#progressBar").hide();
       }
}
    </script>
